#!/usr/bin/expect -f

set force_conservative 0

if {$force_conservative} {
   set send_slow {1 .1}
   proc send {ignore arg} {
      sleep .1
      exp_send -s -- $arg
   }
}

set timeout -1
log_user 0
spawn openssl genrsa -des3 -out SEDkey_fileSED 1024
match_max 100000
expect -exact ":"
send -- "SEDkey_pwdSED\r"
expect -exact ":"
send -- "SEDkey_pwdSED\r"
send_user SEDkey_fileSED
expect eof
